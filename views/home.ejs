<html>
    <head>
        <meta name="viewport" content="width=device-width, initial-scale=1.0">

		<link rel="stylesheet" href="/stylesheets/home.min.css">
		<link rel="stylesheet" type="text/css" href="https://cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.min.css"/>
		<link rel="stylesheet" type="text/css" href="https://cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick-theme.min.css"/>

        <link rel="icon" href="/images/icon 16x16.png" type="image/png">
        <title>CoinTopper</title>
    </head>
    <body>

		<header>
			<nav id="navbar">
				<div id="logo">
					<a href="/"><img src="/images/logo 256x64.png"></a>
					<span id="toggle-icon">
						<span class="icon-bar"></span>
						<span class="icon-bar"></span>
						<span class="icon-bar"></span>
					</span>
				</div>
				<ul>
					<li><a href="/" class="active">Home</a></li>
					<li><a href="#">ICOs</a></li>
					<li><a href="#">Airdrop</a></li>
					<li><a href="#">News</a></li>
					<li><a href="#">Guides</a></li>
					<li><a href="#">Hire Experts</a></li>
					<li><a href="#">Login</a></li>
				</ul>
			</nav>
			<div id="headline">
				<section>
					<div class="headline-item hidden-small">
						<h4>BTC/USD</h4>
						<span>$9120.78</span>
					</div>
					<div class="headline-item hidden-small">
						<h4>Cryptocurrencies</h4>
						<span>1554</span>
					</div>
					<div class="headline-item">
						<h4>24h Volume</h4>
						<span>$137.7 B</span>
					</div>
					<div class="headline-item">
						<h4>AltCoin M.Cap</h4>
						<span>$100.5 B</span>
					</div>
					<div class="headline-item">
						<h4>Total M.Cap</h4>
						<span>$267 B</span>
					</div>
				</section>
			</div>
		</header>

		<main>
			<div class="container">
				<div class="col">
					<div class="row row-between">
						<!-- Title: Top Viewed -->
						<h2 class="title">
						Top Viewed
						</h2>
						<a href="#" class="btn bg-page">View All</a href="#">
					</div>
					<div class="row row-around">
						<!-- Top Viewed Icons -->
						<ul class="top-wrapper">
							<% for(let i=0; i<15; i++) { %>
								<li>
									<a href="#" class="top">
										<span class="top-name">CoinName</span>
										<span class="top-value">$0000.00</span>
										<span class="top-change">
											<img src="/images/equal-light.png" class="change-icon">
											<span class="change-value">0.00%</span>
										</span>
										<img src="/images/btc-light.png" class="top-icon">
									</a>
								</li>
							<% } %>
						</ul>
					</div>
				</div>
				<div class="col">
					<div class="row row-between col-md">
						<h2 class="title">
							<!-- Title: Cryptocurrencies -->
							Cryptocurrencies
						</h2>
						<div class="row row-between">
							<input type="text" placeholder="Search Coin" class="btn bg-page">
							<select name="currency" id="currency" class="btn bg-page">
								<option value="AUD">AUD</option>
								<option value="BRL">BRL</option>
								<option value="CAD">CAD</option>
								<option value="CNY">CNY</option>
								<option value="EUR">EUR</option>
								<option value="GBP">GBP</option>
								<option value="HKD">HKD</option>
								<option value="IDR">IDR</option>
								<option value="INR">INR</option>
								<option value="JPY">JPY</option>
								<option value="KRW">KRW</option>
								<option value="PLN">PLN</option>
								<option value="RUB">RUB</option>
								<option value="TRY">TRY</option>
								<option value="USD" selected>USD</option>
								<option value="ZAR">ZAR</option>
							</select>
						</div>
					</div>
					<div class="row row-between">
						<table class="coin-table">
							<thead>
								<tr>
									<th class="ctd">#</th>
									<th class="ctd"></th>
									<th class="ctd">Name</th>
									<th class="ctd">Change</th>
									<th class="ctd">Price</th>
									<th class="ctd">24h Change</th>
									<th class="ctd">Graph (7 Day)</th>
									<th class="ctd">Coins</th>
									<th class="ctd">Market Cap</th>
								</tr>
							</thead>
							<tbody>
								<% for(let i=1; i<=50; i++) { %>
									<% if(i!=1) { %>
										<tr class="hr"><td colspan="9"></td></tr> 
									<% } %> 
									<tr class="coinrow">
										<td class="ctd"><%= i %></td>
										<td class="ctd">
											<img class="coinicon" src="/images/btc.png">
										</td>
										<td class="ctd">
											<div class="coinname">CoinName</div>
											<div class="coinabbr">CN</div>
										</td>
										<td class="ctd">
											<img src="" class="change-ico">
											<div class="change-val"><img src="/images/equal.png">0.00%</div>
										</td>
										<td class="ctd">
											<div class="price">$0000.00</div>
											<div class="bitval"><img src="/images/btc.png">0.00000000</div>
										</td>
										<td class="ctd">
											<div class="ul"><img src="/images/up.png">$0000</div>
											<div class="ll"><img src="/images/down.png">$0000</div>
										</td>
										<td class="ctd">
											<canvas></canvas>
										</td>
										<td class="ctd">
											<div class="coins">000.00 M</div>
										</td>
										<td class="ctd">
											<div class="mcap">$000.00B</div>
										</td>
									</tr>
								<% } %> 
							</tbody>
						</table>
					</div>
					<div class="row row-center">
						<span class="page">
							<a href="#" class="btn bg-light-border"><< Previous</a>
							<a href="#" class="btn bg-light-border">Next >></a>
						</span>
					</div>
				</div>
				<div class="col">
					<div class="row row-between">
						<!-- Title: News -->
						<h2 class="title">NEWS</h2>
						<a href="#" class="btn bg-page">View All</a href="#">
					</div>
					<div class="row row-around">
						<!-- News Tiles -->
						<ul class="news-wrapper">
							<% for(let i=0; i<9; i++) { %>
								<li>
									<a href="#" class="news">
										<span class="news-fig">
											<img>
											<span class="news-headline">Another Game Changing News</span>
											<span class="datestamp">November 26, 2020</span>
										</span>
									</a>
								</li>
							<% } %>
						</ul>
					</div>
				</div>
			</div>
		</main>

		<footer>
			<div class="links-container">
				<div class="footer-links">
					<div class="footer-1">
						<ul>
							<li class="link-head">CoinTopper</li>
							<a href="#"><li>About</li></a>
							<a href="#"><li>Contact</li></a>
							<a href="#"><li>Advertise</li></a>
							<a href="#"><li>Hire Experts</li></a>
							<a href="#"><li>Terms of Use</li></a>
							<a href="#"><li>Privacy Policy</li></a>
						</ul>
						<ul>
							<li class="link-head">Cryptocurrency</li>
							<a href="#"><li>Bitcoin</li></a>
							<a href="#"><li>Ethereum</li></a>
							<a href="#"><li>Ripple</li></a>
							<a href="#"><li>Bitcoin Cash</li></a>
							<a href="#"><li>Tether</li></a>
							<a href="#"><li class="submit">Submit Coin</li></a>
						</ul>
						<ul>
							<li class="link-head">ICOs</li>
							<a href="#"><li>ACA Network</li></a>
							<a href="#"><li>AgroTechFarm</li></a>
							<a href="#"><li>CRYSTALS</li></a>
							<a href="#"><li>Coin Analyst</li></a>
							<a href="#"><li>IGNOT COIN</li></a>
							<a href="#"><li class="submit">Submit ICO</li></a>
						</ul>
						<ul>
							<li class="link-head">Airdrops</li>
							<a href="#"><li>Tachain</li></a>
							<a href="#"><li>imusify</li></a>
							<a href="#"><li>Bidooh</li></a>
							<a href="#"><li>Spiking</li></a>
							<a href="#"><li>Efforce</li></a>
							<a href="#"><li class="submit">Submit Airdrop</li></a>
						</ul>
					</div>
					<div class="footer-2">
						<ul>
							<li class="link-head">Newsletter</li>
							<li class="newsletter">
								<form>
									<input type="email" placeholder="Your Email" required="true">
									<button><img src="/images/nl.png"></button>
								</form>
							</li>
						</ul>
						<ul>
							<li class="link-head">Download</li>
							<li class="downloads">
								<a target="_blank" href="https://play.google.com/store/apps/details?id=com.cointopper.coinlist"><img src="/images/google-play.png"></a>
								<a target="_blank" href="https://itunes.apple.com/in/app/cointopper/id1384477178?mt=8"><img src="/images/app-store.png"></a>
							</li>
						</ul>
					</div>
				</div>
				<div class="footer-news">
					<ul>
						<li class="link-head">Crypto News and Updates</li>
						<% for(let i=0; i<5; i++) { %> 
							<li class="news-body">
								<a href="#">
									<img src="/images/news-thumb.jpg">
									<div class="news-details">
										<span class="news-head">Another Game Changing news which could...</span>
										<span class="news-date">November 27, 2020</span>
									</div>
								</a>
							</li>
						<% } %> 
					</ul>
				</div>
			</div>
			<div class="end-container">
				<span>© Clone of <a target="_blank" href="https://cointopper.com/" style="text-decoration: none; color: #3c0080;">CoinTopper</a></span>
				<span>Made with ❤️ for the Decentralized World</span>
				<span class="social">
					<a target="_blank" href="https://www.facebook.com/CoinTopper"><img src="/images/fb.png"></a>
					<a target="_blank" href="https://plus.google.com/+CoinTopper"><img src="/images/g+.png"></a>
					<a target="_blank" href="https://twitter.com/CoinTopper"><img src="/images/tw.png"></a>
					<a target="_blank" href="https://www.instagram.com/cointopper/"><img src="/images/ig.png"></a>
					<a target="_blank" href="https://t.me/cointopper"><img src="/images/tg.png"></a>
					<a target="_blank" href="https://www.youtube.com/channel/UCjfel2DFyKFBwcWZ9wfyLUg"><img src="/images/yt.png"></a>
				</span>
			</div>
		</footer>

		<script src="https://code.jquery.com/jquery-3.5.1.min.js" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
		<script type="text/javascript" src="https://cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.min.js"></script>
		<script type="text/javascript" src="/scripts/home.min.js"></script>
	</body>
</html>